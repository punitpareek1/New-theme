<style>

  .inner-get-row-full{
      max-width: 1200px;
    margin: 0 auto;
    text-align: center;
  }
  
  .get-heading {
      font-size: 55px;
    letter-spacing: 4px;
    color: #ffffff;
    font-weight: bold;
    text-align: center;
    font-family: 'sk-modernist';
  }
  
  .outer-get-row-full{
/*   background: #206766; */
    padding: 120px 0;
    background-image: url(https://cdn.shopify.com/s/files/1/2204/9357/files/Color_Fill_19_copy_2.png?v=1650450512);
    background-size: cover;
  }
  
  .get-btn {
      background: #fff;
    padding: 10px 35px;
    border-radius: 40px;
    text-transform: uppercase;
    margin-top: 10px;
    display: inline-block;
    font-size: 18px;
    letter-spacing: 2px;
    color: #000000;
    font-weight: 500;
  }
  .get-btn:hover {
    color: #000;
  }
  
  div#shopify-section-static-newsletter {display: none;}

.get-started-wrapper {padding-bottom: 0px !important;}
  
  .dark-green-waves svg {
  margin-bottom:-10px;
  }
  
  .how-image{
  position: relative;
    z-index: 1;
  }
  
  @media(min-width:1900px){
   .dark-green-waves{
    position: inherit;
    top: 0;
    width: 100%;
  }
    
    .dark-green-waves img {
    width: 100%;
    margin-top: -30px;
  }
    
  	  .dark-green-waves-bottom{
  position: inherit;
    transform: rotate(180deg);
    bottom: 0;
    width: 100%;
  }

.dark-green-waves-bottom img {width: 100%;margin-top: -30px;z-index: 54;}
  
    
  }

  @media(min-width:1400px){
  
    .get-started-wrapper{
      padding: 0;
    background: #fff;
        position: relative;
          margin-top: -100px;
    z-index: 0;
  }
  .dark-green-waves-bottom{
  position: inherit;
    transform: rotate(180deg);
    bottom: 0;
  }
 
    .dark-green-waves{
      position: inherit;
    top: 0;
  }
  
  }
  
  @media(max-width:1399px){
  
    .get-started-wrapper{
      padding: 46px 0;
    background: #fff;
        position: relative;
  }
  .dark-green-waves-bottom {
    position: inherit;
    transform: rotate(180deg);
    margin-top: -2px;
}
    
 .dark-green-waves {
    position: inherit;
    margin-bottom: -2px;
}
    

    
  }
  

  
  @media(max-width:768px){
  
    .dark-green-waves-bottom {
    position: inherit;
    transform: rotate(180deg);
    margin-top: -14px;
}
    .dark-green-waves {
    position: inherit;
    margin-bottom: -14px;
}
    .outer-get-row-full {
    background: #206766;
    padding: 60px 12px;
}
    .get-heading {
    font-size: 28px;
    letter-spacing: 1.5px;
}
    .get-btn {
    font-size: 14px;
    letter-spacing: normal;
    padding: 10px 25px;  
}
    
    .dark-green-waves svg {
    margin-bottom: 3px;
}
    
  }
  
</style>

