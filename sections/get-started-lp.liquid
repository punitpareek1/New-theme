  <div class="outer-get-row-full">

    <div class="inner-get-row-full full-max">

      <div class="content-get-started">
      
        {% if section.settings.heading != blank %}
        <h5 class="get-heading">{{section.settings.heading}}</h5>         
        {% endif %}
        
        {% if section.settings.button_text != blank %}
        <a class="get-btn open-affilate-form" href="{{section.settings.button-url}}">{{section.settings.button_text}} <i class="fa fa-arrow-right" aria-hidden="true"></i></a>         
        {% endif %}
        
      </div>

    </div>
  
  </div>
<!-- <div class="dark-green-waves-bottom"><img src="https://cdn.shopify.com/s/files/1/2204/9357/files/New_Project.png?v=1649419787"></div> -->

</section>


{% schema %}
{
  "name": "Get Started Lp",
  "class": "get-started",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "heading"
    },
    {
      "type": "text",
      "id": "button_text",
	  "label": "Button Text"
    },
    {
      "type": "url",
      "id": "button-url",
      "label": "Button URL"
    }
  ],
	"presets":[{
	"name": "Get Started Lp"
}]
}
{% endschema %}