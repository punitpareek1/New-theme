</header>
<script>
$(document).ready(function () {
		$('.menu-btn').click(function(event) {
			$('.navbar_clinical_header').toggleClass('open-nav');
		});
	});
   document.querySelectorAll('#drawer-navigation .has-dropdown .header-navigation-list-arrow').forEach(btn => {
    btn.addEventListener('click', ()=> {
    btn.closest('.has-dropdown').classList.toggle('open') ; 
  });
  })

  
</script>


