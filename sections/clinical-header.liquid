      </a>
      <a href="{{ section.settings.join_link1 }}" class="join-btn">{{section.settings.join1}} </a>
      {% endif %}
      </div>
      
<div id="clinical_header" class="navbar_clinical_header">
	<div class="nav_bar_clinical_header">

            <div class="navigations-inline">
    
 <nav class="clinical_header_navbar">
  <ul class="clinical_header_navbar_links">

 <label class="dropdown">
