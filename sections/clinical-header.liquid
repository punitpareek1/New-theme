 */

  
</style>

<header style="background: #fff;">

  <div class="header-wrapper">
    
    <div class="header-row main-row-inner-cli">
      {% if section.settings.header_logo != blank %}
      <div class="logos">
        <a class="branding-logo-link" href="https://flore.com/pages/flore-clinical">
        <img src="https://cdn.shopify.com/s/files/1/2204/9357/files/Untitled_1_6673e95a-5853-4612-9e2f-8ea8a1f9a738.png?v=1678880696" width="100%" height="100%" alt="flore_logo"/> 
        </a>
      </div>
      {% endif %}
      
        <div class="join-btn_box Resources_main clinical_hover_wapper"> 
      {% if section.settings.join3 != blank %}
      <a href="{{ section.settings.join_link3 }}" class="join-btn">{{section.settings.join3}} </a>
      {% endif %}
      <span class="icon icon-angle-down header-navigation-list-arrow" data-dropdown-toggle="" tabindex="0" ></span>
    
        <ul class="clinical_hover_inner">
                {% for block in section.blocks %}
          <li class="">
            <a class="" href="{{ block.settings.member_clinical_url }}">{{ block.settings.member_clinical  }}
