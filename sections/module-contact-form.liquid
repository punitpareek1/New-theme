<style>
.hero-contact-container {
  padding: 80px 20px 160px 20px;
  background: url(https://cdn.shopify.com/s/files/1/0605/8177/3565/files/Color_Fill_9.png?v=1649149906);
  background-size: cover;
  background-position: 0px calc(100%);
}
.hero-contact-container h1 {
  text-align: center;
  font-size: 60px;
  color: #FF5C35;
  font-weight: bold;
  font-family: "sk-modernist";
}
/* .hero-contact-container p {
  text-align: center;
  font-size: 60px;
  color: #FF5C35;
  font-weight: bold;
  font-family: "sk-modernist";
} */
.hero-contact-container p {
  max-width: 1100px;
  text-align: center;
  margin: auto;
  color: #292723;
  font-size: 29px;
  line-height: 1.4;
  font-family: "sk-modernist";
  font-weight: 400;
}
.contact-container {
/*   display: grid;
  grid-template-columns: 1.8fr 1fr; */
  gap: 30px;
  max-width: 800px;
  width: 100%;
  padding: 50px 20px;
  margin: auto;
}
.input-wrapper input[type="text"],
.input-wrapper input[type="tel"] {
  border: none;
  border-bottom: 1px solid #00000059;
  border-radius: 0px;
  font-size: 23px;
  padding: 0px;
  color: #000;
}
.input-wrapper input[type="text"]::placeholder,
.input-wrapper input[type="tel"]::placeholder,
.input-wrapper textarea::placeholder {
  /* Chrome, Firefox, Opera, Safari 10.1+ */
  color: #00000059;
  opacity: 1; /* Firefox */
}
.input-wrapper textarea {
  font-size: 23px;
  border: 1px solid #00000059;
  color: #000;
}
.input-wrapper.second {
  width: 46%;
  display: inline-block;
  margin-right: 30px;
}
.input-wrapper.last {
    position: relative;
}
.input-wrapper input.button {
  margin-top: 10px;
  background-color: #FF5C35;
  color: #fff;
  font-size: 21px;
  font-family: "sk-modernist";
  font-weight: 600;
  padding: 15px 50px 15px 40px;
}
.contact-details p {
  font-size: 22px;
  font-weight: 400;
  color: #000;
  line-height: 1;
}
.contact-details p strong {
  font-weight: 700;
}
.contact-details p a {
  color: #000;
}

.input-wrapper.third {
    width: 47%;
    display: inline-block;
}
  
.input-wrapper.last i {
    position: absolute;
    top: 29px;
    left: 140px;
    color: #fff;
    font-size: 16px;
}
  
.contact-details {
    padding-top: 23px;
}  
  
@media (max-width: 700px) {
  .contact-container {
    grid-template-columns: 1fr;
  }

  .hero-contact-container h1 {
    font-size: 40px;
  }
/*  .hero-contact-container p {
    font-size: 40px;
  } */
  .hero-contact-container p {
    font-size: 18px;
  }

  .hero-contact-container {
    padding: 40px 20px 120px;
    background-size: 165% 100%;
    background-position-x: -135px;
  }
  .input-wrapper input.button{
  	font-size:18px;
  }
  .input-wrapper input[type="text"],
  .input-wrapper input[type="tel"],
  .input-wrapper textarea {
    font-size: 18px;
  }

  .input-wrapper.second {
    width: 100%;
    display: block;
  }

  .input-wrapper.third {
    display: block;
    width: 100%;
  }
  .contact-details p{
  	font-size:16px;
  }
  .input-wrapper.last i{
  	left:130px;
  }
  .contact-details {
    padding-top: 0px;
  }
}
#thanks_live{
color: green;
}


  
</style>

<div class="hero-contact-container">
  <h1>{{ section.settings.heading }}</h1>
  <p>{{ section.settings.paragraph }}</p>
</div>

<div class="contact-container">
             <div class="module-text side-text">
              <div class="contact-item contact-form">

<p id="thanks_live"></p>
   
