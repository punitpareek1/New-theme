                {% comment %}
                {% form 'contact', id: 'contact-form' %}
                  {% if form.errors %}
                    <div class="contact-form-error-message">
                      {{ 'contact.error' | t }}
                    </div>
                  {% endif %}
                  <div class="input-wrapper first">
                    <input
                      type="text"
                      id="contact[name]"
                      placeholder="Name"
                      aria-label="{{ 'general.general.name_placeholder' | t }}"
                      value=""
                      name="contact[name]"
                      class="{% if form.errors contains 'author' %}error{% endif %}">
                  </div>
                  <div class="input-wrapper second">
                    <input
                      type="text"
                      id="contact[email]"
                      placeholder="Email"
                      aria-label="{{ 'general.general.email_placeholder' | t }}"
                      value=""
                      name="contact[email]"
                      class="{% if form.errors contains 'email' %}error{% endif %}" required >
                  </div>
                  <div class="input-wrapper third">
                    <input
					  type="tel"
                      id="contact[phone]"
                      placeholder="Phone (optional)"
                      aria-label="{{ 'general.general.phone_placeholder' | t }}"
                      value=""
                      name="contact[phone]"
                      class="{% if form.errors contains 'phone' %}error{% endif %}" required >
                  </div>
                  <div class="input-wrapper">
                    <textarea
                      id="contact[body]"
                      placeholder="Message"
                      aria-label="{{ 'general.general.message' | t }}"
                      name="contact[body]"
                      rows="6"
                      class="{% if form.errors contains 'body' %}error{% endif %}"></textarea>
                  </div>
                  <div class="input-wrapper last">
                    <input
                      class="button outline contact-form-button"
                      type="submit"
                      value="Submit"
                      aria-label="{{ 'general.general.submit' | t }}"
                    >
                    <i class="fa fa-arrow-right" aria-hidden="true"></i>
                  </div>
                  {% if form.posted_successfully? %}
                    <div class="success-message contact-form-success">
                      <span class="icon icon-check contact-form-success-icon"></span>
                    </div>
                  {% endif %}
                {% endform %}
                       {% endcomment %}
                       <div style="text-align: center;"><strong style="font-weight: 500;">Mailing address for kit returns</strong> <br>
                 <h2> 5151 Shoreham Pl, Suite 100, San Diego, CA 92122, United States</h2></div>
              </div>
